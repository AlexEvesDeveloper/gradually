<div class="row wizard-row">    <div class="col-md-12 fuelux">        <div class="block-wizard">            <div id="wizard1" class="wizard wizard-ux">                <ul class="steps">                    <li data-target="#step1" class="active">                        How many?                        <span class="chevron"></span>                    </li>                    <li data-target="#step2">                        Premium jobs?                        <span class="chevron"></span>                    </li>                    <li data-target="#step3">                        CV searches                        <span class="chevron"></span>                    </li>                    <li data-target="#step4">                        Your details                        <span class="chevron"></span>                    </li>                    <li data-target="#step5">                        Payment                        <span class="chevron"></span>                    </li>                </ul>                <div class="actions">                    <button type="button" class="btn btn-xs btn-prev btn-primary">                         <i class="icon-arrow-left"></i>                        Prev                    </button>                    <button type="button" class="btn btn-xs btn-next btn-primary" data-last="Finish">                        Next                        <i class="icon-arrow-right"></i>                    </button>                </div>            </div>            <div class="step-content">                {{form_start(form, {attr:{'class':'form-horizontal', 'role':'form'}})}}                    <div id="step1" class="step-pane active">                        <div class="form-group">                            <div class="col-sm-12">                                <div class="input-group">                                    {{form_label(form.postingCredits, 'How many jobs would you like to post?', {'label_attr':                                        {'class':'control-label'}})                                    }}                                    {{form_widget(form.postingCredits, {attr:                                        {'class':'form-control', 'style':'display:none;'}})                                    }}                                </div>                                <button id="job-credits-1" class="btn btn-xl btn-primary">1</button>                                <button id="job-credits-2" class="btn btn-xl btn-primary">2</button>                                <button id="job-credits-3" class="btn btn-xl btn-primary">3</button>                                <button id="job-credits-4" class="btn btn-xl btn-primary">4</button>                                <button id="job-credits-5" class="btn btn-xl btn-primary">5</button>                                <button id="job-credits-n" class="btn btn-xl btn-primary">5+</button>                            </div>                        </div>                        <div class="form-group">                            <button type="button" class="btn btn-default margin-left-15" data-dismiss="modal">                                Cancel                            </button>                            <button type="button" data-wizard="#wizard1" class="btn btn-primary wizard-next">                                Next Step <i class="fa fa-caret-right"></i>                            </button>                        </div>                    </div>                    <div id="step2" class="step-pane">                        <div class="form-group">                            <div class="col-sm-12">                                <div class="input-group">                                    {{form_label(form.premiumCredits, 'How many of your jobs would you like to be posted as Premium?', {'label_attr':                                        {'class':'control-label'}})                