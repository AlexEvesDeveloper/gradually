{% extends '::base.html.twig' %}

{% block body %}
	<div id="wrapper">
		<div id="sidebar-wrapper">
			<div class="row">
				<div class="col-md-1"></div>
				<div class="col-md-9 under-navbar">
				<img class="thumbnail" src="{{ asset(graduate.image.fullPath) }}" style="width: 200px; height: 200px;" />
				</div>
				<div class="col-md-1"></div>
			</div>
		</div>
		<div id="page-content-wrapper no-top-padding">
				<div class="row under-navbar">
					<div class="col-md-1"></div>
					<div class="col-md-10">
						{% for application in applications %}
          					<h4 class="no-top-padding">
          						<a href="{{ path('gradually_application_graduateapplicationmanager_index', {'id':graduate.id}) }}">
          							{{application.job.title}}
          						</a>
          					</h4>

						{% endfor %}
					</div>
					<div class="col-md-1"></div>
				</div>
		</div>
	</div>
{% endblock %}
