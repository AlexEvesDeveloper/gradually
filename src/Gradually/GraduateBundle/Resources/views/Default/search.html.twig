{% extends "::base.html.twig" %}

{% block body %}
<div class="container under-navbar">
	{% for graduate in graduates %}
		<p>ID: {{ graduate.id }}</p>
		<p>Name: {{ graduate.user.firstname }}</p>
		<p>Qualifications:</p>
		{% for qualification in graduate.qualifications %}
			<ul>
				<li>University: {{ qualification.university.name }}</li>
				<li>Degree: {{ qualification.degree.title }}</li>
                <li>Level: {{ qualification.degreelevel.title }}</li>
                <li>Honors: {{ qualification.result }}</li>
				<li>Year Attained: {{ qualification.yearAttained | date("Y") }}</li>
			</ul>
		{% endfor %}
		<hr>
	{% endfor %}
</div>
{% endblock %}